                        <Button mnemonicParsing="false" onMouseClicked="#addFile" text="新增" />
                        <Button mnemonicParsing="false" onMouseClicked="#removeFile" text="移除" />
                     </children>
                  </HBox>
               </children>
            </VBox>
            <VBox prefHeight="562.0" prefWidth="249.0" spacing="5.0">
